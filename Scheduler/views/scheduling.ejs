			<td id = "spring2" class = "connectedSortable">

			</td>
			<td id = "summer2" class = "connectedSortable">

			</td>
		</tr>
		<tr>
			<td>Junior</td>
			<td id = "autumn3" class = "connectedSortable">

			</td>
			<td id = "winter3" class = "connectedSortable">

			</td>
			<td id = "spring3" class = "connectedSortable">

			</td>
			<td id = "summer3" class = "connectedSortable">

			</td>
		</tr>
		<tr>
			<td>Senior</td>
			<td id = "autumn4" class = "connectedSortable">

			</td>
			<td id = "winter4" class = "connectedSortable">

			</td>
			<td id = "spring4" class = "connectedSortable">

			</td>
			<td id = "summer4" class = "connectedSortable">
			</td>
		</tr>
	</table>
</div>

<script src="/javascripts/js/handlebars-v2.0.0.js"></script>
<script type="text/x-handlebars-template" id="draggable-class-template">
	<div id="course{{databaseId}}" class="list-group-item">
		{{ title }}
		<a href="#" class="remove">&#215;</a> 
	</div>
</script>

